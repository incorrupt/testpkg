{
  "name": "@olivr/testpkg",
  "description": "ddd",
  "version": "0.0.1",
  "author": {
    "name": "romain",
    "email": "romain@b.com",
    "url": "https://b.com"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/incorrupt/testpkg",
  "repository": "github:incorrupt/testpkg",
  "bugs": {
    "url": "https://github.com/incorrupt/testpkg/issues"
  },
  "contributors": [
    {
      "name": "romain",
      "email": "romain@b.com",
      "url": "https://b.com"
    }
  ],
  "directories": {
    "example": "./examples",
    "test": "./tests"
  },
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ],
    "coverageDirectory": ".coverage",
    "testMatch": [
      "**/tests/**/*.[jt]s?(x)"
    ],
    "verbose": true,
    "testEnvironment": "node",
    "projects": [
      {
        "displayName": "browser",
        "testEnvironment": "jsdom"
      },
      {
        "displayName": "node",
        "testEnvironment": "node"
      }
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "babel-eslint": "^10.1.0",
    "eslint": "6.x",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5"
  },
  "scripts": {
    "posttest": "eslint .",
    "test": "jest",
    "format": "eslint . --fix",
    "build": "babel src --out-dir dist",
    "start": "babel src --watch --out-dir dist"
  }
}
